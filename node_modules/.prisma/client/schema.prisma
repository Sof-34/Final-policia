datasource db {
  provider = "mysql"
  url      = env("MYSQL_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Denuncia {
  iddenuncias  Int                   @id @default(autoincrement())
  tipo         String
  nombre       String
  email        String
  telefono     String
  genero       String
  descripcion  String
  seguimientos Denunseguimipolicia[]
}

model Denunseguimipolicia {
  id               Int      @id @default(autoincrement())
  iddenuncia       Int
  descripcion      String
  fechaSeguimiento DateTime @default(now())
  estado           String
  Denuncia         Denuncia @relation(fields: [iddenuncia], references: [iddenuncias])
}
